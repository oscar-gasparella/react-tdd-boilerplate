{
  "setupFiles": ["<rootDir>/tests/setup.js"],
  "moduleFileExtensions": ["ts", "tsx", "js"],
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "testMatch": ["<rootDir>/tests/unit/**/*.(ts|tsx)"],
  "snapshotSerializers": ["enzyme-to-json/serializer"],
  "moduleNameMapper": {
    "^.+\\.(css|less)$": "<rootDir>/tests/stubs/CSSStub.js"
  }
}
